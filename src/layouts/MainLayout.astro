---
import "../styles/global.css";
import MainHead from "./MainHead.astro";
import MainFooter from "./MainFooter.astro";
const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
    <MainHead {title} />
    <body>
        <main>
            <slot />
        </main>
    </body><MainFooter />
</html>

<style>
    html,
    body {
        margin: 0;
        width: 100%;
        height: 100%;
    }
</style>
